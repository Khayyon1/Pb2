{
   "name" : "Tungsten",
	"version" : "1.0",
	"description" : "Writing assistant for browsers",
   "background": {
      "scripts": ["scripts/background.js"],
      "persistent": false
   },

   "browser_action":
   {
      "default_popup": "popup.html",
   },
   "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["scripts/main.js"]
   }
 ],
	"manifest_version" : 2
}
